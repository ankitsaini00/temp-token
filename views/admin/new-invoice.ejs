    <style>
        @charset "UTF-8";
        .flex {
            display: flex;
            flex-direction: row;
            justify-content: space-around;
        }
        
        .flex h1 {
            color: #18191b;
            margin-bottom: 2rem;
        }
        
        .flex input[type=radio] {
            display: none;
        }
        
        .flex input[type=radio]:not(:disabled)~label {
            cursor: pointer;
        }
        
        .flex input[type=radio]:disabled~label {
            color: #bcc2bf;
            border-color: #bcc2bf;
            box-shadow: none;
            cursor: not-allowed;
        }
        
        .flex label {
            height: 100%;
            display: block;
            background: white;
            border: 2px solid #1ba3e1;
            border-radius: 10px;
            padding: 15px 20px 0px 20px;
            text-align: center;
            box-shadow: 0px 3px 10px -2px rgba(161, 170, 166, 0.5);
            position: relative;
        }
        
        .flex input[type=radio]:checked+label {
            background: #1ba3e1;
            color: white;
            box-shadow: 0px 0px 10px #1ba3e1;
        }
        
        .flex input[type=radio]:checked+label::after {
            color: #000000;
            font-family: FontAwesome;
            border: 2px solid #1ba3e1;
            content: "✓";
            font-size: 20px;
            position: absolute;
            top: -20px;
            left: 50%;
            transform: translateX(-40%);
            height: 30px;
            width: 30px;
            line-height: 30px;
            text-align: center;
            border-radius: 50%;
            background: white;
            box-shadow: 0px 2px 5px -2px rgba(0, 0, 0, 0.25);
        }
        
        .flex p {
            font-weight: 900;
        }
        
        @media only screen and (max-width: 700px) {
            .flex section {
                flex-direction: column;
            }
        }
        
        .a {
            display: inline-block;
        }
        
        .a h6 {
            color: #000000;
        }
        
        .a input[type=radio] {
            display: none;
        }
        
        .a input[type=radio]:not(:disabled)~label {
            cursor: pointer;
        }
        
        .a input[type=radio]:disabled~label {
            color: #bcc2bf;
            border-color: #bcc2bf;
            box-shadow: none;
            cursor: not-allowed;
        }
        
        .a label {
            height: 100%;
            display: block;
            background: white;
            border: 2px solid #1ba3e1;
            border-radius: 10px;
            padding: 10px 10px 0px 10px;
            text-align: center;
            box-shadow: 0px 3px 10px -2px rgba(161, 170, 166, 0.5);
