                function changedroptype(el) {
                    if (el.value == 0) {
                        $('.iphone').css('display', 'none');
                        $('.iwatch').css('display', 'none');
                        $('.airpod').css('display', 'none');

                    } else if (el.value == 1) {
                        $('.iphone').css('display', 'block');
                        $('.iwatch').css('display', 'none');
                        $('.airpod').css('display', 'none');
                    } else if (el.value == 2) {
                        $('.iwatch').css('display', 'block');
                        $('.iphone').css('display', 'none');
                        $('.airpod').css('display', 'none');
                    } else if (el.value == 3) {
                        $('.airpod').css('display', 'block');
                        $('.iphone').css('display', 'none');
                        $('.iwatch').css('display', 'none');
                    }
                }
            </script>
