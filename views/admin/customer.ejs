    <style>
        .flash,
        .flash2 {
            background-color: rgb(236, 0, 0);
            border-radius: 5px;
            padding: 20px 30px;
            left: 30%;
            right: 30%;
            position: fixed;
            top: 3px;
            z-index: 15;
            font-size: 1.3rem;
            color: white;
        }

        .flash .close1,
        .flash2 .close1 {
            float: right;
            color: white;
            cursor: pointer;
            margin-left: 10px;
        }

        .innerflash {
            display: flex;
            align-items: center;
        }

        .flash .i,
        .flash2 .i {
            font-size: 2rem;
            color: white;
            float: left;
            margin-right: 20px;
        }

        .flash2 {
            background-color: rgb(0, 172, 0);
        }

        @media screen and (max-width: 900px) {

            .flash,
            .flash2 {
                font-size: 0.9rem;
                padding: 12px 25px;
                margin: 3px;
                left: 0;
                right: 0;
            }

            .flash .i,
            .flash2 .i {
                font-size: 1.5rem;
            }
        }
    </style>

